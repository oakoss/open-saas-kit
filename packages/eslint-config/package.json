{
  "name": "@oakoss/eslint-config",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./base": "./base.js",
    "./react": "./react.js"
  },
  "scripts": {
    "clean": "rm -rf node_modules",
    "lint": "eslint . --max-warnings 0 --cache --cache-location node_modules/.cache/eslint/.eslintcache",
    "lint:fix": "eslint . --fix --max-warnings 0 --cache --cache-location node_modules/.cache/eslint/.eslintcache",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^2.7.4",
    "@eslint/js": "^9.39.2",
    "@oakoss/typescript-config": "workspace:^",
    "@tanstack/eslint-plugin-query": "^5.91.3",
    "@tanstack/eslint-plugin-router": "^1.155.0",
    "@types/node": "^25.0.10",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-better-tailwindcss": "^4.0.2",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-keys-plus": "^1.5.0",
    "eslint-plugin-turbo": "^2.7.6",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^17.1.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript": "^5.0.0"
  }
}
