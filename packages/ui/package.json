{
  "name": "@oakoss/ui",
  "version": "0.0.0",
  "private": true,
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./theme.css": "./dist/styles/theme.css",
    "./utils": "./src/lib/utils.ts"
  },
  "scripts": {
    "build": "tsdown",
    "build-storybook": "storybook build --stats-json",
    "clean": "rm -rf node_modules dist coverage",
    "dev": "tsdown --watch './src/**/*'",
    "lint": "eslint . --max-warnings 0 --cache --cache-location node_modules/.cache/eslint/.eslintcache",
    "lint:fix": "eslint . --fix --max-warnings 0 --cache --cache-location node_modules/.cache/eslint/.eslintcache",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.8",
    "@remixicon/react": "^4.8.0",
    "class-variance-authority": "^0.7.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "next-themes": "^0.4.6",
    "react-aria-components": "^1.14.0",
    "react-day-picker": "^9.13.0",
    "react-resizable-panels": "^4.4.1",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@bosh-code/tsdown-plugin-tailwindcss": "^1.0.1",
    "@chromatic-com/storybook": "^5.0.0",
    "@oakoss/eslint-config": "workspace:^",
    "@oakoss/typescript-config": "workspace:^",
    "@storybook/addon-a11y": "^10.2.0",
    "@storybook/addon-docs": "^10.2.0",
    "@storybook/addon-themes": "^10.2.0",
    "@storybook/addon-vitest": "^10.2.0",
    "@storybook/react-vite": "^10.2.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "@vueless/storybook-dark-mode": "^10.0.6",
    "eslint": "^9.39.2",
    "eslint-plugin-storybook": "^10.2.0",
    "playwright": "^1.58.0",
    "shadcn": "^3.7.0",
    "storybook": "^10.2.0",
    "storybook-addon-deep-controls": "^0.10.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-react-aria-components": "^2.0.1",
    "tsdown": "0.20.0-beta.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}
